<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="title">
        <item name="android:textColor">@color/cam</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/_23ssp</item>
    </style>

    <style name="content">
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">@dimen/_14ssp</item>
    </style>

    <style name="divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?attr/dividerHorizontal</item>
    </style>

    <style name="customfontstyle" parent="@android:style/TextAppearance.Small">
        <item name="android:fontFamily">@font/montserrat_regular</item>
    </style>

    <style name="specific_rounded_corner">
        <item name="cornerSizeTopLeft">@dimen/_25sdp</item>
        <item name="cornerSizeBottomRight">@dimen/_25sdp</item>
        <item name="cornerSizeTopRight">@dimen/_25sdp</item>
<!--        <item name="cornerFamilyTopLeft">rounded</item>-->
<!--        <item name="cornerFamilyBottomRight">rounded</item>-->
    </style>



</resources>